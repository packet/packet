packet Simple {
  @size uint8 x;
}

packet SimpleParent {
  char c;
  @size uint8 l;
}

@type_selector(c=1)
packet AnotherSimple(SimpleParent) {
  Simple y;
}

@type_selector(c=3)
packet YetAnotherSimple(SimpleParent) {
  @size(simples) int8 s;
  @repeated Simple simples;
}

@type_selector(c=4)
packet YetYetAnotherSimple(SimpleParent) {
  @size(simples) int8 s;
  @repeated int8 simples;
}


